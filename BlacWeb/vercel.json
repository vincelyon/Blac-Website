{
    "builds": [{
        "src": "BlacWeb/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "25mb",
            "runtime": "python3.10"
        }
    }],
    "routes": [
        {
            "src": "/static/(.*)", 
            "dest": "/static/$1"   
        },
        {
            "src": "/media/(.*)", 
            "dest": "/media/$1"  
        },
        {
            "src": "/(.*)",
            "dest": "BlacWeb/wsgi.py"
        }
    ]
}
